#include <stdio.h>
int main() {
	int n,i,t;
  t=0;
  scanf("%d",&n);
  int a[n];
  int b[n];
  int c[n];
  for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
  {
    scanf("%d",&b[i]);
  }
  for(i=0;i<n;i++)
  {
    c[i]=a[i] + b[i];
  }
  for(i=0;i<n;i++)
    printf("%d ",c[i]);
     
	return 0;
}
